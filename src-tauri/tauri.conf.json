{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "devtool-manager",
  "version": "0.2.1",
  "identifier": "com.administrator.devtool-manager",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1422",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "DevTool Manager",
        "width": 1200,
        "height": 800,
        "minWidth": 900,
        "minHeight": 600,
        "center": true,
        "resizable": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "plugins": {
    "updater": {
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IEIyMkExQjk1NTQ3RDI2REMKUldUY0puMVVsUnNxc2pLS1ArWDB5ZjVqalJtbFJUMlBuaElFL3pWWklXWCtFSFBqWkEvM0NDb3IK",
      "endpoints": [
        "https://github.com/dengyuwu/dev-tools/releases/latest/download/latest.json"
      ]
    }
  }
}
